{
  "public": "out",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/**", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "**/*.@(jpg|jpeg|gif|png|webp|svg|ico|css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}